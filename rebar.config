{erl_opts, [warn_unused_import, warn_export_vars, verbose, report, debug_info]}.

{deps, [{katana_code, "~> 2.1.0"}]}.

{project_plugins, [{rebar3_format, "~> 1.3.0"}]}.

{format,
 [{files,
   ["priv/**/*.?rl",
    "src/**/*.app.src",
    "src/**/*.erl",
    "test/*.erl",
    "test/files/**/*.erl",
    "*.config"]}]}.
